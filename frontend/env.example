# Frontend Environment Variables
# Copy this file to .env.local and update the values

# Backend API URL
NEXT_PUBLIC_API_URL=http://127.0.0.1:8000

# Frontend URL (optional)
NEXT_PUBLIC_FRONTEND_URL=http://localhost:3000

# Analytics (optional)
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ANALYTICS_DOMAIN=localhost
NEXT_PUBLIC_ANALYTICS_SCRIPT_URL=https://plausible.gonzalochale.dev/js/script.outbound-links.js

# Note: All environment variables must be prefixed with NEXT_PUBLIC_ 
# to be accessible in the browser

NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_FEEDBACK=true
NEXT_PUBLIC_ENABLE_NOTIFICATIONS=true
NEXT_PUBLIC_ENABLE_SERVICE_WORKER=true
NEXT_PUBLIC_ENABLE_OFFLINE_SUPPORT=true